{
  "name": "myinvestor-challenge",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Full-stack funds management application with Express API and React frontend",
  "main": "dist/server/app.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node --watch server/app.ts",
    "dev:client": "vite",
    "start": "node --watch server/app.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:coverage:open": "vitest --coverage && start coverage/index.html",
    "test:threshold": "vitest --coverage --reporter=verbose",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.7.7",
    "body-parser": "^2.2.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.64.0",
    "react-router-dom": "^7.9.4"
  },
  "engines": {
    "yarn": "^4.5.1",
    "node": "^24.5.0"
  },
  "packageManager": "yarn@4.5.1",
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.8",
    "vitest": "^3.2.4"
  }
}